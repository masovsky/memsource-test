<?xml version="1.0" encoding="UTF-8"?>
<configuration debug="false" packagingData="true">
	<statusListener class="ch.qos.logback.core.status.NopStatusListener" />

	<appender name="default" class="ch.qos.logback.core.ConsoleAppender" withJansi="true">
		<encoder>
			<pattern>%d{yyyy-MM-dd HH:mm:ss.SSS, UTC} [%.15thread] %-5level %logger{20} - %msg%n%ex{50}</pattern>
		</encoder>
	</appender>

	<root level="DEBUG">
		<appender-ref ref="default"/>
	</root>

	<logger name="memsource" level="DEBUG"/>

</configuration>
